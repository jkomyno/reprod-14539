generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Post {
  id Int @id
}

model Vacancy {
  id Int @id @default(autoincrement())
  staffMemberId Int?
  staffMember StaffMember? @relation(fields: [staffMemberId], references: [id])
}

model StaffMember {
  id        Int @id @default(autoincrement())
  name      String
  vacancies Vacancy[]
}
